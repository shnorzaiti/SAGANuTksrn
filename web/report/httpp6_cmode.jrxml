	<queryString>
		<![CDATA[select  'T'||h.maj_mjkod||lpad(a.peg_akaun,7,'0')||lpad(a.peg_digit,2,'0') bkaun2,
            'SALINAN' salinan, f.htx_nobil nobil2,
            TO_CHAR(htx_tkhbl, 'dd/mm/yyyy') tkhbl2,
            a.peg_akaun pegakaun2,
            a.peg_adpg1 adpg12, d.jln_jnama adpg22, d.jln_poskd adpg32,
            a.peg_nilth nilth2, c.kaw_kadar kadar2,
            round(nvl(a.peg_nilth,0)*nvl(c.kaw_kadar,0)/100,2) tksir2,
            f.htx_statf statf2, f.htx_tmpoh tmpoh2,

	  case when (to_number(to_char(htx_tkhbl,'MM')) > 06) then
            'SEPARUH TAHUN KEDUA'||'  '||to_number(to_char(htx_tkhbl,'YYYY'))
            else
            'SEPARUH TAHUN PERTAMA'||' '||to_number(to_char(htx_tkhbl,'YYYY'))
            end tempoh,
            'SEPARUH TAHUN' tempoh1,
            case when  (to_number(to_char(htx_tkhbl,'MM')) > 06) then
            'JUL - DIS'||'  '||to_number(to_char(htx_tkhbl,'YYYY'))
            else
            'JAN - JUN'||'  '||to_number(to_char(htx_tkhbl,'YYYY'))
            end tempoh2,

            i.pvd_pnama pnama2, i.pvd_almt1 adpk12,
            i.pvd_almt2 adpk22, i.pvd_almt3 adpk32,
            (select mnmsej||' '||mnmsj1||' '||mnmsj2 from  mtkn.msggpr) mesej

from        mtkn.hlpgng a, mtkn.hlpmlk b, mtkn.hkawkd c, spbt.vjalan d,
            mtkn.hjenpk e, mtkn.hlhtrx f, spbt.majlis h, spbt.vplngn i
where   b.pmk_akaun = a.peg_akaun and a.peg_akaun = f.htx_akaun (+)
and     a.peg_jlkod = d.jln_jlkod
and     d.jln_kwkod = c.kaw_kwkod
and     a.peg_akaun = i.pvd_akaun
and     b.pmk_akaun = i.pvd_akaun
and     b.pmk_plgid = i.pvd_plgid
and     i.pvd_modin = 'T'
and     a.peg_htkod = c.kaw_htkod
and     a.peg_jpkod = e.jpk_jpkod and a.peg_jlkod = d.jln_jlkod
and     e.jpk_itkod = '11010'
and     a.peg_statf  <> 'H'
and     a.peg_tkhpl = to_date($P{param1}, 'dd/mm/yyyy')
order by a.peg_akaun]]>
	</queryString>
