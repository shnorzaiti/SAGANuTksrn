<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="htawal_a4_amaun_del" language="groovy" pageWidth="310" pageHeight="840" columnWidth="310" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="dc6886ba-d4a1-43a7-ab78-38a4a9674f64">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="PARAM1" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select 	trx_level, decode(SPBT.EPBT.GET_PENGGAL(ADD_MONTHS(sysdate, 6)),1,nvl(TKSR2,0),nvl(TKSR1,0)) amun,
 	PEG_AKAUN tkaun,
          	trx_inama bnama
from    	spbt.trxkod, (select PEG_AKAUN, TKSR1, TKSR2
from mtkn.HVNDUK4) induk
where 	trx_itkod = '11010' and trx_modin = 'T'
and  PEG_AKAUN = $P{PARAM1}
UNION ALL
select 	trx_level, sum(nvl(htx_amaun,0)) tmaun, htx_akaun tkaun,
          	trx_inama tnama
from    	mtkn.hlhtrx, spbt.trxkod
where 	substr(htx_kdrjk,3,3) = substr(trx_itkod,3,3)
and 	trx_itkod IN ('12010','12011','11011','12040','12041','12042','12015') and trx_modin = 'T'
and 	nvl(htx_amaun,0) <> 0
and     htx_akaun = $P{PARAM1}
group by 	trx_level, htx_akaun, trx_inama
order by trx_level]]>
	</queryString>
	<field name="TRX_LEVEL" class="java.lang.String"/>
	<field name="AMUN" class="java.math.BigDecimal"/>
	<field name="TKAUN" class="java.math.BigDecimal"/>
	<field name="BNAMA" class="java.lang.String"/>
