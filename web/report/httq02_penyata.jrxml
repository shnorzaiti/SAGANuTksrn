	<queryString>
		<![CDATA[select  pyt_akaun akaun2, pyt_tkhbl tkhbl,
        pyt_nobil nobil, pyt_keter inama,
        decode(substr(pyt_kdrjk,1,1),'1',pyt_amaun,null) debit,
        decode(substr(pyt_kdrjk,1,1),'2',pyt_amaun,null) credt,
        pyt_amaun amaun
 from   mtkn.hvpnyt
 where  pyt_akaun = $P{PARAM1} and
        ((pyt_stats = $P{PARAM2}) or
         (pyt_statf <> 'T' and $P{PARAM2} = '99'))
 order by   pyt_tkhbl, substr(pyt_kdrjk,1,1),
            substr(pyt_kdrjk,2,1) desc,
            substr(pyt_kdrjk,3,3)]]>
	</queryString>
