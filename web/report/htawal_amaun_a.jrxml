	</parameter>
	<queryString>
		<![CDATA[select 	TRX_LEVEL, decode(SPBT.EPBT.GET_PENGGAL(spbt.epbt.get_tempoh_bil_awal(sysdate)),1,nvl($P{PARAM3},0),nvl($P{PARAM2},0)) tmaun,
 	SPBT.EPBT.ISNUMBER($P{PARAM1}) tkaun,
          	trx_inama tnama
from    	spbt.trxkod
where 	trx_itkod = '11010' and trx_modin = 'T'
UNION
select  TRX_LEVEL, sum(nvl(htx_amaun,0)) tmaun,
            htx_akaun tkaun,
            trx_inama tnama
from        mtkn.hlhtrx, spbt.trxkod
where   substr(htx_kdrjk,3,3) = substr(trx_itkod,3,3)
and    trx_itkod IN ('12010','12020','12030','12040','12041','12042','12015') and trx_modin = 'T'
and htx_akaun = $P{PARAM1}
and     nvl(htx_amaun,0) <> 0
group by TRX_LEVEL, htx_akaun, trx_inama
ORDER BY TRX_LEVEL]]>
	</queryString>
	<field name="TRX_LEVEL" class="java.lang.String"/>
	<field name="TMAUN" class="java.math.BigDecimal"/>
	<field name="TKAUN" class="java.math.BigDecimal"/>
	<field name="TNAMA" class="java.lang.String"/>
