	<queryString>
		<![CDATA[select  'T'||lpad(peg_akaun,7,'0')||'-'||lpad(peg_digit,2,'0') noakn,
    pmk_akaun akaun, NVL(peg_oldac,'-') oldac, pmk_plgid plgid,
    pmk_nmbil nmbil, jln_jnama jnama, jln_poskd poskd,
    nvl(nobil,'-')NOBIL, nvl(kdrjk,'-') KDRJK, nvl(tkhbl,'')tkhbl,
    peg_adpg1 adpg1, nvl(peg_nilth,0) nilth, kaw_kadar kadar,
    round(nvl(peg_nilth,0)*nvl(kaw_kadar,0)/100*
    pmk_prtus/100,2) tksir,
    nvl(ambyr,0)ambyr,
    nvl(ambil,0)ambil,
    nvl(inama,0)inama,
    nvl(tmpoh,0)tmpoh,
    nvl(hbaki,0)hbaki,
    ((nvl(ambil,0)-nvl(ambyr,0)))jbaki,
    spbt.epbt.get_para_value('MTKN','1')mnama

from         mtkn.hlpmlk,mtkn.hlpgng,mtkn.hkawkd,spbt.vjalan,

(select  pyt_akaun, decode(substr(pyt_kdrjk,1,1),2,pyt_amaun,0) ambyr,
                decode(substr(pyt_kdrjk,1,1),1,pyt_amaun,0) ambil,
                pyt_nobil nobil, pyt_kdrjk kdrjk, pyt_tkhbl tkhbl,
    pyt_statf statf, trx_inama inama,
    sysdate - pyt_tkhbl tmpoh,
                (decode(substr(pyt_kdrjk,1,1),'1',pyt_amaun,0)-
            decode(substr(pyt_kdrjk,1,1),'2',pyt_amaun,0)) hbaki
from         mtkn.hvpnyt,spbt.trxkod
where      trx_itkod = pyt_kdrjk and trx_modin = 'T' and (pyt_stats = $P{PARAM1})
order by   6)


where   ((peg_akaun between $P{PARAM2} and $P{PARAM3})
and pmk_akaun = pyt_akaun (+)
and  (peg_jlkod between $P{PARAM4} and $P{PARAM5}))
and          peg_akaun = pmk_akaun and peg_jlkod = jln_jlkod
and          kaw_kwkod = jln_kwkod and kaw_htkod = peg_htkod
order by   peg_akaun, tkhbl]]>
	</queryString>
