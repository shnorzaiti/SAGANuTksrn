	<queryString>
		<![CDATA[select pyt_tkhbl tkhby, pyt_nobil nobyr,
       sum(decode(substr(pyt_kdrjk,1,4),2104,pyt_amaun,0)) byrsms,
       sum(decode(substr(pyt_kdrjk,1,4),2204,pyt_amaun,0)) byrtgk,
       sum(decode(substr(pyt_kdrjk,1,4),2104,pyt_amaun,0))+
       sum(decode(substr(pyt_kdrjk,1,4),2204,pyt_amaun,0)) jmbyr
from mtkn.hlpnyt
where pyt_tkhbl >= $P{tkhnt}
and pyt_akaun = $P{akaun}
and pyt_statf = 'R'
and substr(pyt_kdrjk,1,5) in ('21040','22040')
group by pyt_akaun, pyt_tkhbl, pyt_nobil]]>
	</queryString>
