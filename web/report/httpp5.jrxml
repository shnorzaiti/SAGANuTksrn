	<queryString>
		<![CDATA[select  distinct
    'T'||maj_mjkod||lpad(peg_akaun,7,0)||'-'||peg_digit noakn,
            peg_akaun akaun, pmk_nmbil pnama,
            pvd_almt1 adpk1, pvd_almt2 adpk2, pvd_almt3 adpk3,
    pvd_almt4 adpk4, pvd_almt5 adpk5,
            adpg1, adpg2, adpg3, adpg4
,(select para_value

    from   spbt.std_sys_param
    where  para_id = 54)keter8,(select para_value

    from   spbt.std_sys_param
    where  para_id = 55)keter9, (select para_value
	from spbt.std_sys_param
	where para_id = 57) keter10,
(select para_value
	from MTKN.std_sys_param
	where para_id = 101) tempoh
from    	mtkn.hvnduk4, spbt.majlis, mtkn.hlhtrx
where 	pmk_akaun = peg_akaun
and 	jln_kwkod = kaw_kwkod and peg_htkod = kaw_htkod
and 	peg_jlkod = jln_jlkod
and	peg_akaun = htx_akaun
and 	peg_jpkod = jpk_jpkod
and 	jpk_itkod = '11010'
and 	(peg_statf  <> 'H' or peg_statf is null)
and 	((peg_akaun between $P{param1} and $P{param2})
and 	 (peg_jlkod between $P{param3} and $P{param4}))
having 	sum(htx_amaun) between $P{param5} and $P{param6}
group by 	maj_mjkod, peg_akaun, peg_digit, pmk_nmbil, pvd_almt1,
	pvd_almt2, pvd_almt3, pvd_almt4, pvd_almt5,  adpg1, adpg2, adpg3, adpg4
order by 	peg_akaun]]>
	</queryString>
