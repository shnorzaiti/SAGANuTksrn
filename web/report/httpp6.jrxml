	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["D:\\ireport\\mtkn\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="PARAM1" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select distinct 'T-'||maj_mjkod||lpad(acm_akaun,7,'0')||'-'||lpad(acm_digit,2,'0') fkaun,
                ('T'||maj_mjkod||lpad(acm_akaun,7,0)||lpad(acm_digit,2,'0')) bkaun,
                case when (SPBT.epbt.get_penggal(sysdate) = 2) then
                'SEPARUH TAHUN KEDUA'||'  '||to_number(to_char(bc1_tkhbl,'YYYY'))
                else
                'SEPARUH TAHUN PERTAMA'||'  '||to_number(to_char(bc1_tkhbl,'YYYY'))
                end tajukBil,
                'BIL TERKINI' salinan1,
                acm_akaun akaun,
                pmk_nmbil pnama, pvd_almt1 almt1,
                pvd_almt2 almt2, pvd_almt3 almt3,
                pvd_almt4 almt4, pvd_almt5 almt5,
                bc1_nobil nobil, peg_oldac oacct,
                to_char(bc1_tkhbl, 'dd/mm/yyyy') tkhbl,
                acm_nilth nilth, kaw_kadar kadar,
                peg_tksir tksir, tksr1,
                round(nvl(acm_nilth,0)*nvl(kaw_kadar,0)/100,2)/2 tksir2,
                adpg1, adpg2,
                adpg3||decode(adpg4,null,null,', '||adpg4) adpg3,
                case when  (to_number(to_char(htx_tkhbl,'MM')) > 06) then
                'JUL - DIS'||'  '||to_number(to_char(htx_tkhbl,'YYYY'))
                else
                'JAN - JUN'||'  '||to_number(to_char(htx_tkhbl,'YYYY'))
                end tempoh1,
                bc1_tmpoh tmpoh,
                bc1_statf statf,
                adpg1||', '||adpg2||', '||adpg3||decode(adpg4,null,null,', '||adpg4) almat,
               (select mnmsej||' '||mnmsj1||' '||mnmsj2 from  mtkn.msggpr) mesej,
                tmaun jmlhBil, spbt.epbt.get_bundar(tmaun) lmaun,
                tmaun + spbt.epbt.get_bundar(tmaun) jmlah,
                to_char(tmaun + spbt.epbt.get_bundar(tmaun)) bjmlah
from mtkn.hvnduk4, mtkn.hlkbc1, spbt.majlis, mtkn.hlhtrx, mtkn.vcmacm,
   (select sum(nvl(htx_amaun,0)) tmaun,
               htx_akaun tkaun
    from mtkn.hlhtrx, spbt.trxkod
    where substr(htx_kdrjk,1,5) = trx_itkod
    and substr(htx_kdrjk,1,5) in ('11010','12010','11020','12020','11030','12030','11040','12040',
                                  '11041','12041','11042','12042','11015','12015')
    and trx_modin = 'T' and (nvl(htx_amaun,0) > 0 or nvl(htx_amaun,0) < 0)
    group by htx_akaun) transaksi
where peg_akaun = bc1_akaun (+)
and peg_akaun = htx_akaun
and peg_akaun = acm_akaun
and peg_akaun = tkaun (+)
and jpk_itkod = '11010'
and peg_tksir >= (select para_value from mtkn.std_sys_param where para_id = 20)
and acm_tkhpl = to_date ($P{PARAM1},'DD/MM/YYYY')
and acm_jenis in ('B','C')
order by acm_akaun]]>
	</queryString>
