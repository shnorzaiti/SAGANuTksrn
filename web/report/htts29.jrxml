	<queryString>
		<![CDATA[select mkm_mkkod||' '||mkm_mnama mukim, kws_kwkod||' '||kws_knama kwsan,
       jln_jlkod||' '||jln_jnama jalan, noakn, peg_akaun akaun, pmk_nmbil nmbil,
       peg_adpg1 adpg1, jln_jnama adpg2, jln_poskd adpg3, jln_negri adpg4,
       spbt.epbt.get_para_value('spbt',1) mnama,
       peg_htang
from mtkn.hvnduk4,
    (select htx_akaun htxakaun,
            nvl(sum(htx_amaun),0)
     from mtkn.hlhtrx
     where htx_amaun > 0
     group by htx_akaun)
where peg_akaun = htxakaun
and peg_akaun between $P{PARAM1} and $P{PARAM2}
and jln_jlkod between $P{PARAM3} and $P{PARAM4}
order by mkm_mkkod, kws_kwkod, jln_jlkod, peg_akaun]]>
	</queryString>
