	<queryString>
		<![CDATA[select  peg_akaun akaun1,
        'T-'||maj_mjkod||lpad(peg_akaun,7,0)||'-'||lpad(peg_digit,2,0) noakn,
        peg_oldac oldac, pmk_nmbil pnama, adpg1, adpg2,
        adpg3, adpg4, pmk_plgid nomkp,
        peg_nilth nilth, kaw_kadar kadar,
        peg_tksir tksir, CASE WHEN ($P{PARAM5} = '99') THEN 99 ELSE TO_NUMBER(TO_CHAR(SYSDATE,'yyyy'), '9999') - TO_NUMBER($P{PARAM5}, '99') END tahun,
        jmlhPenyata.debit, jmlhPenyata.credt

from    mtkn.hvnduk2, spbt.majlis, (select pyt_akaun,
        sum(decode(substr(pyt_kdrjk,1,1),'1',pyt_amaun,null)) debit,
        sum(decode(substr(pyt_kdrjk,1,1),'2',pyt_amaun,null)) credt
 from   mtkn.hvpnyt
 where
        ((pyt_stats = $P{PARAM5}) or
         (pyt_statf <> 'T' and $P{PARAM5} = '99')) GROUP BY pyt_akaun) jmlhPenyata
where  (peg_akaun between $P{PARAM1} and $P{PARAM2})
  AND PEG_AKAUN = pyt_akaun]]>
	</queryString>
