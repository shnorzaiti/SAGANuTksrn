	<queryString>
		<![CDATA[select distinct  'T-'||maj_mjkod||lpad(peg_akaun,7,'0')||'-'||lpad(peg_digit,2,'0') fkaun,
                ('T'||maj_mjkod||lpad(peg_akaun,7,0)||lpad(peg_digit,2,'0')) bkaun,
                 peg_akaun akaun,
          SPBT.EPBT.get_keter_penggal(sysdate) tajukBil,
                 'ANSURAN' salinan1, pmk_nmbil pnama,
                 pvd_almt1 almt1, pvd_almt2 almt2,
                 pvd_almt3 almt3,  pvd_almt4 almt4,
                 pvd_almt5 almt5,
                 peg_nilth nilth, kaw_kadar kadar,
                 peg_tksir tksir,
                 adpg1 adpg11, adpg2 adpg22, adpg3 adpg33, adpg4 adpg44,
                 adpg1||', '||adpg2||', '||adpg3||decode(adpg4,null,null,', '||adpg4) almat,
                 TO_CHAR(in2_trikh, 'dd/mm/yyyy') tkhbl,
                 in2_nobil nobil, peg_oldac oacct, 'BIL ANSURAN '||rownum || ' DARIPADA ' || (select in1_jmseq from mtkn.HLINS1 where IN1_AKAUN = $P{PARAM1}) mesej
from    mtkn.hvnduk4, mtkn.hlins2, spbt.majlis
where   peg_akaun = in2_akaun (+)
and     jpk_itkod = '11010'
and     nvl(peg_statf,'H')  <> 'H'
and     (peg_akaun = $P{PARAM1})
order by peg_akaun, mesej]]>
	</queryString>
