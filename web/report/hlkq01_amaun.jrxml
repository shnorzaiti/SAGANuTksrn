	<field name="TNAMA" class="java.lang.String"/>
	<field name="TMAUN" class="java.math.BigDecimal"/>
	<variable name="TMAUN" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{TMAUN}]]></variableExpression>
	</variable>
