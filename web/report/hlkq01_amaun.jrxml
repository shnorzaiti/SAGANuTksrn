<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="hlkq01_amaun" language="groovy" pageWidth="317" pageHeight="840" columnWidth="317" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="dc6886ba-d4a1-43a7-ab78-38a4a9674f64">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="PARAM1" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="PARAM2" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select tkaun, tnama, tmaun
from (
WITH     col_cntr AS
(
 SELECT     column_name, column_id
     FROM     all_tab_columns
     WHERE     owner          = 'MTKN'
     AND     table_name     = 'HLINS2'
)
  SELECT  -- TRX_LEVEL,
          	a.IN2_akaun tkaun,
          spbt.epbt.get_keterangan_kod ('TRXKOD', 'T'||CASE c.column_name
              WHEN  'IN2_CTSMS'  THEN  '11010'
              WHEN  'IN2_CTTGK'  THEN  '12010'
              WHEN  'IN2_DNSMS'  THEN  '11030'
              WHEN  'IN2_DNTGK'  THEN  '12030'
              WHEN  'IN2_NTSMS'  THEN  '11020'
              WHEN  'IN2_NTTGK'  THEN  '12020'
              WHEN  'IN2_WRSMS'  THEN  '11040'
              WHEN  'IN2_WRTGK'  THEN  '11040'
            --  WHEN  'IN2_LNSMS'  THEN  a.IN2_LNSMS
            --  WHEN  'IN2_LNTGK'  THEN  a.IN2_LNTGK
          END) tnama -- c.column_name
     ,     CASE c.column_name
              WHEN  'IN2_CTSMS'  THEN  a.IN2_CTSMS
              WHEN  'IN2_CTTGK'  THEN  a.IN2_CTTGK
              WHEN  'IN2_DNSMS'  THEN  a.IN2_DNSMS
              WHEN  'IN2_DNTGK'  THEN  a.IN2_DNTGK
              WHEN  'IN2_NTSMS'  THEN  a.IN2_NTSMS
              WHEN  'IN2_NTTGK'  THEN  a.IN2_NTTGK
              WHEN  'IN2_WRSMS'  THEN  a.IN2_WRSMS
              WHEN  'IN2_WRTGK'  THEN  a.IN2_WRTGK
           --   WHEN  'IN2_LNSMS'  THEN  a.IN2_LNSMS
           --   WHEN  'IN2_LNTGK'  THEN  a.IN2_LNTGK
          END tmaun
  FROM    MTKN.HLINS2  a, col_cntr c
  WHERE   a.in2_akaun = $P{PARAM1} and
          a.in2_trikh = spbt.epbt.isdate($P{PARAM2},'dd/mm/yyyy') and
          c.column_id BETWEEN 13 AND 22
ORDER BY  c.column_id
	)
	where tmaun is not null]]>
	</queryString>
