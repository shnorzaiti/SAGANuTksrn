	<queryString>
		<![CDATA[select bgn_bgkod, bgn_bnama,
       nvl(bgn_cukai,0) bgn_cukai,
       nvl(bgn_bycuk,0) bgn_bycuk,
       nvl(bgn_tngak,0) bgn_tngak,
       nvl(bgn_bytgk,0) bgn_bytgk,
       spbt.epbt.get_para_value('spbt',1) mnama,
       jbilgn
from mtkn.jumbgn,
    (select peg_bgkod, count(peg_bgkod) jbilgn
     from mtkn.hlpgng
     group by peg_bgkod)
where bgn_bgkod = peg_bgkod
order by bgn_bgkod ]]>
