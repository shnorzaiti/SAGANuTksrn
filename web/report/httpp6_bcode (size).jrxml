	<queryString>
		<![CDATA[select  'T'||h.maj_mjkod||lpad(a.peg_akaun,7,'0')||lpad(a.peg_digit,2,'0') bkaun2,
            'SALINAN' salinan, b.htx_nobil nobil2,
            TO_CHAR(htx_tkhbl, 'dd/mm/yyyy') tkhbl2,
            a.peg_akaun pegakaun2,
            a.peg_adpg1 adpg12, jln_jnama adpg22, jln_poskd adpg32,
            a.peg_nilth nilth2, kaw_kadar kadar2,
            round(nvl(a.peg_nilth,0)*nvl(kaw_kadar,0)/100,2) tksir2,
            b.htx_statf statf2, b.htx_tmpoh tmpoh2,
           case when (to_number(to_char(htx_tkhbl,'MM')) > 06) then
            'SEPARUH TAHUN KEDUA'||'  '||to_number(to_char(htx_tkhbl,'YYYY'))
            else
            'SEPARUH TAHUN PERTAMA'||' '||to_number(to_char(htx_tkhbl,'YYYY'))
            end tempoh,
            'SEPARUH TAHUN' tempoh1,
            case when  (to_number(to_char(htx_tkhbl,'MM')) > 06) then
            'JUL - DIS'||'  '||to_number(to_char(htx_tkhbl,'YYYY'))
            else
            'JAN - JUN'||'  '||to_number(to_char(htx_tkhbl,'YYYY'))
            end tempoh2,
            pvd_pnama pnama2, pvd_almt1 adpk12,
            pvd_almt2 adpk22, pvd_almt3 adpk32,
           (select mnmsej||' '||mnmsj1||' '||mnmsj2 from  mtkn.msggpr) mesej,
            tmaun jmlhBil, spbt.epbt.get_bundar(tmaun) lmaun,
            tmaun + spbt.epbt.get_bundar(tmaun) jmlah,
                '*'||to_char(tmaun + spbt.epbt.get_bundar(tmaun))||'*' bjmlah
from mtkn.hvnduk4 a, mtkn.hlhtrx b, spbt.majlis h,
    (select sum(nvl(htx_amaun,0)) tmaun,
              htx_akaun tkaun
     from mtkn.hlhtrx, spbt.trxkod
     where  substr(htx_kdrjk,1,5) = trx_itkod
     and substr(htx_kdrjk,1,5) in ('11010','12010','11020','12020','11030','12030','11040','12040',
                                  '11041','12041','11042','12042','11015','12015')
     and trx_modin = 'T' and (nvl(htx_amaun,0) > 0 or nvl(htx_amaun,0) < 0)
group by htx_akaun) transaksi
where a.peg_akaun = b.htx_akaun (+)
and peg_akaun = tkaun (+)
and a.jpk_itkod = '11010' and a.peg_statf  <> 'H'
and peg_tksir >= (select para_value from mtkn.std_sys_param where para_id = 20)
and a.peg_tkhpl = to_date($P{PARAM1}, 'dd/mm/yyyy')
order by a.peg_akaun]]>
	</queryString>
