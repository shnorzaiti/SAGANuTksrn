	<queryString>
		<![CDATA[select distinct  'T-'||maj_mjkod||lpad(peg_akaun,7,'0')||'-'||lpad(peg_digit,2,'0') fkaun,
                ('T'||maj_mjkod||lpad(peg_akaun,7,0)||lpad(peg_digit,2,'0')) bkaun,
                 peg_akaun akaun,
          SPBT.EPBT.get_keter_penggal(sysdate) tajukBil,
                 'BIL ASAL' salinan1, pmk_nmbil pnama,
                 pvd_almt1 almt1, pvd_almt2 almt2,
                 pvd_almt3 almt3,  pvd_almt4 almt4,
                 pvd_almt5 almt5,
                 peg_nilth nilth, kaw_kadar kadar,
                 peg_tksir tksir,
                 adpg1 adpg11, adpg2 adpg22, adpg3 adpg33, adpg4 adpg44,
                 adpg1||', '||adpg2||', '||adpg3||decode(adpg4,null,null,', '||adpg4) almat,
                 TO_CHAR(bc1_tkhbl, 'dd/mm/yyyy') tkhbl, TO_CHAR(bc1_tmpoh,'dd/mm/yyyy') tmpoh,
           SPBT.EPBT.get_tempoh_bil (sysdate) tempoh,
                 bc1_nobil nobil, bc1_tkhbl tkhbl, bc1_tmpoh tmpoh,
                 bc1_statf statf, peg_oldac oacct,
                 (select mnmsej||' '||mnmsj1||' '||mnmsj2 from   mtkn.msggpr) mesej,
           tmaun jmlhBil, SPBT.EPBT.GET_BUNDAR(tmaun) lmaun,
           spbt.epbt.get_para_value('MTKN',71) jstat,
decode(spbt.epbt.get_para_value('MTKN',71),'T',null,spbt.epbt.get_para_value('MTKN',70)) biller,
decode(spbt.epbt.get_para_value('MTKN',71),'T',null,'T'||maj_mjkod||lpad(peg_akaun,7,'0')||lpad(peg_digit,2,'0')) nopay,
decode(spbt.epbt.get_para_value('MTKN',71),'T',null,spbt.epbt.get_para_value('MTKN',72)) kepay,
decode(spbt.epbt.get_para_value('MTKN',71),'T',null,spbt.epbt.get_para_value('MTKN',74)) ref2,
spbt.epbt.get_para_value('MTKN',73) refJ, nvl(jmlbh,0) jmlbh
from    mtkn.hvnduk4, mtkn.hlkbc1, spbt.majlis,
(select bc2_akaun, sum (nvl (bc2_amaun, 0)) jmlbh
          from mtkn.hlkbc2, spbt.trxkod
         where substr (bc2_kdrjk, 3, 3) = substr (trx_itkod, 3, 3)
           and trx_itkod in ('11090')
           and trx_modin = 'T'
           and nvl (bc2_amaun, 0) < 0
        group by bc2_akaun),
(select sum(nvl(bc2_amaun,0)) tmaun,
            bc2_akaun tkaun
from        mtkn.hlkbc2, spbt.trxkod
where   substr(bc2_kdrjk,1,5) = trx_itkod
and     substr(bc2_kdrjk,1,2) IN ('11','12') and trx_modin = 'T'
and     (nvl(bc2_amaun,0) > 0 or nvl(bc2_amaun,0) < 0)
group by BC2_AKAUN) transaksi
where   peg_akaun = bc1_akaun (+)
and   peg_akaun = bc2_akaun (+)
and     peg_akaun = tkaun (+)
and     substr(jpk_itkod,1,2) = '11'
and     nvl(peg_statf,'H')  <> 'H'
and     peg_tksir >= (select para_value from mtkn.std_sys_param where para_id = 20)
and     (peg_akaun between $P{PARAM1} and $P{PARAM2})
AND (jln_jlkod between $P{PARAM3} and $P{PARAM4})
and (spbt.epbt.isnumber(replace(nvl(peg_htang,0),',','')) between spbt.epbt.isnumber($P{PARAM5}) and spbt.epbt.isnumber($P{PARAM6}))
AND (jln_pskod between $P{PARAM7} and $P{PARAM8})
order by peg_akaun]]>
	</queryString>
