	<queryString>
		<![CDATA[select  nvl(a.htx_amaun,0) amaun,
            a.htx_akaun akaun1,
            b.trx_inama bnama1
from        mtkn.hlhtrx a, spbt.trxkod b
where   substr(a.htx_kdrjk,1,5) = b.trx_itkod
and     substr(a.htx_kdrjk,1,5) IN ('12042','12041','12040','12030','12020','12010','11040','11030','11020','11010')
and     b.trx_modin = 'T'
and a.htx_akaun between $P{param1} and $P{param2}
and     (nvl(a.htx_amaun,0) > 0 or nvl(a.htx_amaun,0) < 0)]]>
	</queryString>
